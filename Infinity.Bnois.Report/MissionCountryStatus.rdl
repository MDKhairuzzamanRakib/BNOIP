  <DataSets>
    <DataSet Name="MissionCountryStatus">
      <Query>
        <DataSourceName>MissionCountryStatus</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmployeeId">
            <Value>=Parameters!EmployeeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Country.PhoneCode
FROM            vwTransfer LEFT OUTER JOIN
                         Country ON vwTransfer.CountryId = Country.CountryId
WHERE        (vwTransfer.TransferFor = 3) AND (vwTransfer.EmployeeId = @EmployeeId)</CommandText>
      </Query>
      <Fields>
